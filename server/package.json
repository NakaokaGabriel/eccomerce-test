{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "nodemon src/index.ts",
    "start:prod": "node dist/index.js",
    "build": "tsc --build",
    "test": "jest",
    "test:live": "jest --watch",
    "test:liveAll": "jest --watchAll",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "migrate:run": "./migrations/postgresql/manage_migrations.sh run",
    "migrate:status": "./migrations/postgresql/manage_migrations.sh status",
    "migrate:create": "./migrations/postgresql/manage_migrations.sh create",
    "migrate:reset": "./migrations/postgresql/manage_migrations.sh reset",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose -f docker-compose.migrations.yml up --build",
    "docker:down": "docker-compose down -v"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^5.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}